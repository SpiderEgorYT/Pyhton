q_2 = 6
orng = ["#ffb70070","#ffb7008c","#ffb700b0","#faca50","white"]
q_1 = 0
pendown()
for a in range(5):
    width(q_2)
    color(orng[q_1])
    fd(500)
    goto(-250,-10)
    q_2 = q_2 - 1
    q_1 = q_1 + 1
    






